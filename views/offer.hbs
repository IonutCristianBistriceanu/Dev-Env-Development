{{> top}}
<div class="container">
    <div class="row">
        <div class="col-lg-4 p-5">
            <h4>Location: {{house.location}}</h4>
            <h6 style="word-wrap: break-word" class>Description: {{house.description}}</h6>
            <h6>Contact person: {{email}}</h6>
        </div>

        <div class="col-xl-8 p-5">
            <img class="card-img-top img-responsive" src="/uploads/{{house.imageName}}" alt="Card image cap">
        </div>
    </div>
</div>

{{> bottom}}
